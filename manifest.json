{
  "manifest_version": 2,
  "name": "Phishing Detector Extension",
  "version": "1.0",
  "description": "Extension to detect whether a given URL is legitimate or not",
  "permissions": [
    "tabs",
    "storage",
    "webNavigation",
    "webRequest",
    "<all_urls>"
  ],
  "background": {
    "scripts": ["background.js"]
  },
  "browser_action": {
    "default_popup": "popup.html"
  },
  "web_accessible_resources": ["warning.html"]
}
